<resources>
    <declare-styleable name="ButtonAttrs">
        <attr name="mainButtonColor" format="color" />
        <attr name="secondaryButtonColor" format="color" />
        <attr name="pressedButtonColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="CustomGoogleButtonView">
        <attr name="logoSize" format="dimension" />
